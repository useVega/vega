{
  "name": "@vega-protocol/client",
  "version": "0.1.0",
  "description": "Frontend client library for the Vega Protocol - Build, execute, and monitor multi-agent workflows",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "vega",
    "protocol",
    "workflow",
    "multi-agent",
    "ai-agents",
    "workflow-engine",
    "yaml-parser",
    "agent-orchestration",
    "a2a-protocol",
    "web3",
    "blockchain"
  ],
  "author": "Vega Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vega-protocol/client"
  },
  "bugs": {
    "url": "https://github.com/vega-protocol/client/issues"
  },
  "homepage": "https://github.com/vega-protocol/client#readme",
  "dependencies": {
    "yaml": "^2.5.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
